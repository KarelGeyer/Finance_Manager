{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "MailConfig": {
    "Address": "",
    "Smtp": "",
    "Port": "587",
    "ShouldUseSSL": false,
    "SSL": "",
    "Username": "",
    "Password": ""
  },
  "AllowedHosts": "*"
}
